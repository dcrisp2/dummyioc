##-------------------------##
#	DEVICE I_CSET/RSET/RD	#
##-------------------------##
record(ao, "$(sys)_$(subsys):$(dev)_$(inst):$(sig)_CSET")
{
	field(DESC, "$(desc)")
	field(EGU,  "$(egu)")
	field(DRVL, "$(drvl)")
	field(DRVH, "$(drvh)")
	field(PREC, "3")
	field(PINI, "YES")
	field(ADEL, "0.01")
	info(autosaveFields, "VAL")
}
record(ai, "$(sys)_$(sub):$(dev)_$(inst):$(sig)_RSET")
{
	field(DESC, "$(desc)")
	field(EGU, "$(egu)")
	field(PREC, "3")
	field(PINI, "0")
	field(ADEL, "0.01")
}
record(ai, "$(sys)_$(sub):$(dev)_$(inst):$(sig)_RD")
{
	field(DESC, "$(desc)")
	field(EGU, "$(egu)")
	field(PREC, "3")
	field(HHSV, "MAJOR")
	field(HSV, "MINOR")
	field(LSV, "MINOR")
	field(LLSV, "MAJOR")
	field(PINI, "0")
	field(ADEL, "0.01")
}

##-------------------------##
#	PS* binary io			#
##-------------------------##
record(bo, "$(sys)_$(subsys):$(dev)_$(inst):ON_CMD")
{
	field(DESC, "On Command")
	field(VAL, "0")
	field(OUT, "$(sys)_$(subsys):$(dev)_$(inst):ON_RSTS NPP MS")
	field(PINI, "0")
}

record(bi, "$(sys)_$(subsys):$(dev)_$(inst):ON_RSTS")
{
	field(DESC, "On Command")
	field(VAL, "0")
	field(PINI, "1")
}

record(bo, "$(sys)_$(subsys):$(dev)_$(inst):RST_CMD")
{
	field(DESC, "Reset Interlock Command")
	field(VAL, "0")
	field(OUT, "$(sys)_$(subsys):$(dev)_$(inst):ILK_RSTS NPP MS")
	field(PINI, "1")
}

record(bi, "$(sys)_$(subsys):$(dev)_$(inst):ILK_RSTS")
{
	field(DESC, "Interlock Read Status")
	field(VAL, "0")
	field(PINI, "1")
}

